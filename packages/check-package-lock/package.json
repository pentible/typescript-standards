{
    "name": "@pentible/check-package-lock",
    "version": "0.104.0",
    "description": "command to verify a package-lock.json is in sync with its package.json",
    "repository": {
        "type": "git",
        "url": "https://github.com/pentible/typescript-standards.git"
    },
    "license": "MIT",
    "author": "pentible",
    "type": "commonjs",
    "imports": {
        "#src/*": [
            "./src/*.js",
            "./src/*.ts"
        ]
    },
    "main": "./dist/main.js",
    "source": "./src/main.ts",
    "bin": {
        "check-package-lock": "./dist/main.js"
    },
    "files": [
        "dist/main.js"
    ],
    "scripts": {
        "build": "esbuild --bundle --platform=node --target=node18 --minify --external:node-gyp --outfile=./dist/main.js src/main.ts",
        "dev": "npm run build -- --watch",
        "preinstall": "mkdir -p dist && touch dist/main.js",
        "start": "./dist/main.js"
    },
    "devDependencies": {
        "@npmcli/arborist": "7.2.1",
        "@types/minimatch": "3.0.5",
        "@types/node": "18.19.22",
        "@types/npmcli__arborist": "5.6.11",
        "@types/semver": "7.5.8",
        "@types/yargs": "17.0.32",
        "chalk": "4.1.2",
        "execa": "8.0.1",
        "minimatch": "3.1.2",
        "semver": "7.6.0",
        "yargs": "17.7.2",
        "zod": "3.22.4"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    }
}
